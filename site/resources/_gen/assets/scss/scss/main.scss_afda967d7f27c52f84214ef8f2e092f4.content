html,body{height:100%;width:100%;margin:0}.background{position:absolute;top:0;left:0;right:0;height:100%;max-height:220px;object-fit:cover;width:inherit;z-index:-1}@media (min-width: 768px){.background{height:unset;max-height:unset;object-fit:unset}}main{width:inherit;padding-top:30vw}@media (min-width: 576px){main{padding-top:20vw}}main header{display:flex;justify-content:space-between;align-items:center;width:90vw;max-width:1500px;height:25vh;min-height:175px;max-height:400px;margin-top:0;margin-bottom:40px;margin-left:auto;margin-right:auto;background:#ffffff;box-shadow:0px 4px 10px rgba(0,0,0,0.25)}@media (min-width: 1200px){main header{min-height:300px}}main header h1{margin:0 0 0 20px;font-family:"Roboto Slab", serif;font-style:normal;font-weight:bold;font-size:2em;line-height:50px;color:#000000}@media (min-width: 1200px){main header h1{font-size:4em;line-height:95px}}main header img{height:100%;display:none}@media (min-width: 768px){main header img{display:block}}main .info-card h2{margin:0;font-family:"Roboto Slab", serif;font-style:normal;font-weight:normal;font-size:20px;line-height:47px;color:black;text-align:center}@media (min-width: 1200px){main .info-card h2{font-size:2em}}main .info-card hr{margin-top:30px;border-width:0;border-top-width:0px;border-top:1px solid black;width:20%;max-width:100px}main .info-card .links{font-family:"Roboto Slab", serif;font-size:20px;margin-top:10px;color:black;text-align:center}main .info-card .links a{text-decoration:none;line-height:35px;color:black;padding:0 8px}main .info-card .links a:hover{color:#434343}
